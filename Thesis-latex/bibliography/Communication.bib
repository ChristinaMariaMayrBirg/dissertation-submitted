@InProceedings{dittrich-2009-com,
 abstract = {Dynamic navigation systems can substantially benefit from reliable real-time urban traffic information. In this paper a method is shown that calculates the current urban traffic state based on probe vehicle data from fleet vehicles. On all links where there is no information present, historical traffic data is used. The calculated velocity information is integrated into the dynamic on-board navigation system. In contrast to other routing solutions this method not only informs the driver about traffic congestion and dense traffic, but also about currently free-flowing traffic. The authors conclude by comparing the probe vehicle data approach to existing car-2-car based traffic information systems.},
 author = {Dittrich, Claudia and Leonhardt, Axel and Menig, Cornelius and Wischhof, Lars},
 booktitle = {Proc. of 16th ITS World Congress and Exhibition on Intelligent Transport Systems and Services},
 date = {2009},
 title = {On-Board Urban Traffic State Based on Probe Vehicle Data}
