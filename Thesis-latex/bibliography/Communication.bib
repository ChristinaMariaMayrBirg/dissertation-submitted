@InProceedings{wischhof-2020-com,
  author    = {Lars Wischhof and Fabian Schaipp and Stefan Schuhb\"{a}ck and Gerta K\"{o}ster},
  booktitle = {Procedings of the IEEE International Symposium on Personal, Indoor and Mobile Radio Communications (PIMRC 2020)},
  date      = {2020-09},
  title     = {Simulation vs. Testbed: Small Scale Experimental Validation of an Open-Source LTE-A Model},
  doi       = {10.1109/PIMRC48278.2020.9217163},
  abstract  = {A majority of scientific publications on wireless
networking is (partly) based on network simulation in order to
evaluate the presented concepts, algorithms and architectures.
In research related to 4G and 5G cellular communication, the
system-level simulation SimuLTE is a widely used model. Despite
its popularity, no results regarding an experimental validation of
this model are publicly available up to now. This paper presents
results of a first step in this direction: A small-scale completely
shielded cellular testbed based on OpenAirInterface is set up
and packet delay and inter-arrival time are measured in several
low and high load scenarios. Afterwards, the identical scenarios
are modeled in SimuLTE. The comparison of simulation and
measurement results indicate a good accuracy of the model for
low-load scenarios. However, in particular in overload conditions
involving more than one packet source the results differ
significantly. Subsequently, several adaptations to the model are
proposed which lead to a better agreement of simulation and
measurement results in the investigated scenarios.},
  comment   = {Antrag:resolut},
  file      = {:Articles/wischhof-2020-com.pdf:PDF},
  keywords  = {measurement, prototype, testbed, simulation},
}

