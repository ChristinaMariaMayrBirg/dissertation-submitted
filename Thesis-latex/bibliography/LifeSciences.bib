@Article{sheikhnejad-2022-life,
  author    = {Sheikhnejad, Yahya and Aghamolaei, Reihaneh and Fallahpour, Marzieh and Motamedi, Hamid and Moshfeghi, Mohammad and Mirzaei, Parham A. and Bordbar, Hadi},
  file      = {sheikhnejad-2022-life.pdf:Articles\\sheikhnejad-2022-life.pdf:PDF},
  journal   = {Sustainable Cities and Society},
  keywords  = {CFD, review},
  owner     = {sr},
  timestamp = {2023-11-21},
  title     = {Airborne and aerosol pathogen transmission modeling of respiratory events in buildings: {A}n overview of computational fluid dynamics},
  year      = {2022},
  doi       = {10.1016/j.scs.2022.103704},
  pages     = {103704},
  volume    = {79},
  abstract  = {Pathogen droplets released from respiratory events are the primary means of dispersion and transmission of the recent pandemic of COVID-19. Computational fluid dynamics (CFD) has been widely employed as a fast, reliable, and inexpensive technique to support decision-making and to envisage mitigatory protocols. Nonetheless, the airborne pathogen droplet CFD modeling encounters limitations due to the oversimplification of involved physics and the intensive computational demand. Moreover, uncertainties in the collected clinical data required to simulate airborne and aerosol transport such as droplets' initial velocities, tempo-spatial profiles, release angle, and size distributions are broadly reported in the literature. There is a noticeable inconsistency around these collected data amongst many reported studies. This study aims to review the capabilities and limitations associated with CFD modeling. Setting the CFD models needs experimental data of respiratory flows such as velocity, particle size, and number distribution. Therefore, this paper briefly reviews the experimental techniques used to measure the characteristics of airborne pathogen droplet transmissions together with their limitations and reported uncertainties. The relevant clinical data related to pathogen transmission needed for postprocessing of CFD data and translating them to safety measures are also reviewed. Eventually, the uncertainty and inconsistency of the existing clinical data available for airborne pathogen CFD analysis are scurtinized to pave a pathway toward future studies ensuing these identified gaps and limitations.},
  publisher = {Elsevier BV},
}

@Article{sedighi-2023-life,
  author       = {Sedighi, Ali Asghar and Haghighat, Fariborz and Nasiri, Fuzhan and Cao, Shijie and Ren, Chen},
  file         = {sedighi-2023-life.pdf:Articles\\sedighi-2023-life.pdf:PDF},
  owner        = {sr},
  timestamp    = {2023-11-21},
  title        = {Approaches in {CFD} modeling of respiratory droplet dispersion - issues and challenges},
  year         = {2023},
  doi          = {10.1016/j.scs.2023.104696},
  journal      = {Sustainable Cities and Society},
  pages        = {104696},
  volume       = {97},
  abstract     = {Airborne viruses can be transmitted from infected person to healthy one through small respiratory droplets. Comprehensive information on the dispersion pattern of saliva droplets in indoor environment can be very helpful in finding ways to control its transmission. Various approaches, including experimental and numerical techniques, have been suggested to understand and study the behavior of indoor droplet dispersion. Computational Fluid Dynamics (CFD) is known as one of the affordable and inexpensive method for modeling droplet dispersion. Although CFD methods have been advanced significantly in recent years, researchers have not reached a general conclusion on the CFD modeling of respiratory droplets dispersion, considering its complex mechanisms through speaking, coughing, sneezing and breathing. A systematic review of CFD techniques employed to study the dispersion of droplets and droplet nuclei is performed in this article. The advantages and disadvantages of each approach for numerical simulation of the droplet dispersion are discussed in order to find the most suitable method in CFD simulation of respiratory droplet dispersion.},
  publisher    = {Elsevier BV},
}

