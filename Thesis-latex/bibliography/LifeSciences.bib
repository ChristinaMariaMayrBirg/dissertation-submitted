@article{adenzato-2006-life,
 abstract = {In recent years, neurophysiological and psychological research has highlighted a pragmatic version of the theory of knowledge, a version in which the concept of simulation has been found to play a crucial role. In fact, research on canonical and mirror neurons has shown that an as if simulative schema is required to perceive, categorize and understand the meaning of the external world. The present study compares the cognitive paradigm of embodied cognition with Pierre Bourdieu's practice theory. Specifically, cognitive processes and cultural mechanisms are described as phenomena that emerge from the dynamic interaction that exists between people's practical abilities and the structure of the local environment in which they act and live. A pragmatic conception of knowledge has also emerged in the field of ethnological investigation. Indeed, the concepts of resonance and empathy have proven to be essential instruments for ethnological knowledge.},
 author = {Adenzato, Mauro and Garbarini, Francesca},
 doi = {10.1177/0959354306070515},
 file = {adenzato-2006-life.pdf:Articles\\adenzato-2006-life.pdf:PDF},
 journal = {Theory \& Psychology},
 keywords = {cognition},
 number = {6},
 pages = {747--759},
 title = {The As If in Cognitive Science, Neuroscience and Anthropology: A Journey among Robots, Blacksmiths and Neurons},
 volume = {16},
 year = {2006}
}

@article{aiello-1977-life,
 abstract = {An experiment was performed to examine the role of interpersonal distance preference as a mediator of short-term crowding involving close interpersonal proximity. It was expected that subjects with far interpersonal distance preferences would experience greater stress-related arousal in a crowded environment than subjects preferring closer interpersonal distances and consequently would display more physiological, behavioral, and subjective indications of discomfort. During the crowding experience, subjects preferring far interpersonal distances were found to be most physiologically stressed (as measured by electrodermal responses) and later reported having experienced greater somatic stress. Crowded subjects had higher skin conductance levels during exposure, thereafter reported greater discomfort due to restricted space, and manifested lower performance levels on creativity tasks administered immediately following crowding. Results are discussed in the context of the loss of established control processes and the breakdown of interaction-regulation mechanisms.},
 author = {John R. Aiello and Donna T. DeRisi and Yakov M. Epstein and Robert A. Karlin},
 journal = {Sociometry},
 keywords = {psychology, personal space},
 month = {09},
 number = {3},
 pages = {271--282},
 title = {Crowding and the Role of Interpersonal Distance Preference},
 volume = {40},
 year = {1977}
}

@inproceedings{andrighetto-2007-life,
 abstract = {In this paper we will present the EMIL project, "EMergence In the Loop: Simulating the two-way dynamics of norm innovation", a three-year project funded by the European Commission (Sixth Framework Programme -Information Society and Technologies) in the framework of the initiative "Simulating Emergent Properties in Complex Systems". The EMIL project intends to contribute to the study of social complex systems by modelling norm innovation as a phenomenon implying interrelationships among multiple levels. It shall endeavour to point out that social dynamics in societies of intelligent agents is necessarily bi-directional, which adds complexity to the emergence processes. The micro-macro link will be modelled and observed in the emergence of properties at the macro-level and their immergence into the micro-level units. The main scientific aim of the EMIL project is to construct a simulator for exploring and experimenting norm-innovation.},
 address = {Dagstuhl, Germany},
 author = {Andrighetto, Giulia and Conte, Rosaria and Turrini, Paolo and Paolucci, Mario},
 booktitle = {Normative Multi-agent Systems},
 editor = {Boella, Guido and van der Torre, Leon and Verhagen, Harko},
 file = {andrighetto-2007-life.pdf:Misc\\andrighetto-2007-life.pdf:PDF},
 issn = {1862-4405},
 keywords = {modeling, cognition, norm, behavior},
 number = {07122},
