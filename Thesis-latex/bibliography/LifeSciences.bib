@Article{zhu-2020b-life,
  author       = {Na Zhu and Dingyu Zhang and Wenling Wang and Xingwang Li and Bo Yang and Jingdong Song and Xiang Zhao and Baoying Huang and Weifeng Shi and Roujian Lu and Peihua Niu and Faxian Zhan and Xuejun Ma and Dayan Wang and Wenbo Xu and Guizhen Wu and George F. Gao and Wenjie Tan},
