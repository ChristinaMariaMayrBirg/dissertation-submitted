@Article{harweg-2023b-cdyn,
  author    = {Thomas Harweg and Daniel Bachmann and Frank Weichert},
  file      = {harweg-2023b-cdyn.pdf:Articles\\harweg-2023b-cdyn.pdf:PDF},
  journal   = {Journal of Public Health},
  keywords  = {agent-based, disease, covid-19, SARS-CoV-2},
