@Article{chraibi-2015-cdyn,
  author   = {M. Chraibi and T. Ensslen and H. Gottschalk and M. Saadi and A. Seyfried},
  title    = {Assessment of Models for Pedestrian Dynamics with Functional Principal Component Analysis},
  doi      = {10.1016/j.physa.2016.01.058},
