@article{molnar-2001-cdyn,
 abstract = {Self-organized and error-resistant control of distributed autonomous robotic units in a manufacturing environment with obstacles where the robotic units have to be assigned to manufacturing targets in a cost effective way, is achieved by using two fundamental principles of nature. First, the selection behavior of modes is used which appears in pattern formation of physical, chemical and biological systems. Coupled selection equations based on these pattern formation principles can be used as dynamical system approach to assignment problems. These differential equations guarantee feasibility of the obtained solutions which is of great importance in industrial applications. Second, a model of behavioral forces is used, which has been successfully applied to describe self-organized crowd behavior of pedestrians. This novel approach includes collision avoidance as well as error resistivity. In particular, in systems where failures are of concern, the suggested approach outperforms conventional methods in covering up for sudden external changes like breakdowns of some robotic units. The capability of this system is demonstrated in computer simulations.},
 author = {P\'{e}ter Moln\'{a}r and Jens Starke},
 comment = {SFM for robots, but e_ij destination vector of the robotic units is determined by a set of coupled selection equations with the dynamic variables. Uses mollification of directional vector, but with typo in it. Later publications by Starke will no longer determine dv/dt = something * directional vector but dr/dt = sth.* dir. v., ust as we replace the SFM by the GNM.},
 file = {molnar-2001-cdyn.pdf:Articles\\molnar-2001-cdyn.pdf:PDF},
 journal = {IEEE TRANSACTIONS ON SYSTEMS, MAN, AND CYBERNETICS?PART B: CYBERNETICS},

 number = {3},
