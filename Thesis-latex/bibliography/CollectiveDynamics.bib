 journal = {Physics Letters A},
 keywords = {modeling, behavior, psychology, floor field, pedestrian, density},
 number = {35},
 owner = {gk},
 pages = {2445--2454},
 title = {An extended model for describing pedestrian evacuation under the threat of artificial attack},
 volume = {382},
 year = {2018}
}

