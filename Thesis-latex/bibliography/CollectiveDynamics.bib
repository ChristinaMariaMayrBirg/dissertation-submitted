@article{subramanian-2021-cdyn,
author = {Subramanian, Gayathri Harihara and Karthika, P.S. and Verma, Anish},
title = {Macroscopic fundamental flow diagrams of a spiritually motivated crowd},
journal = {Advances in Complex Systems},
volume = {24},
