@article{dsilva-2016-math,
    author = "Dsilva, Carmeline J. and Talmon, Ronen and Gear, C. William and Coifman, Ronald R. and Kevrekidis, Ioannis G.",
    title = "Data-{Driven} {Reduction} for a {Class} of {Multiscale} {Fast}-{Slow} {Stochastic} {Dynamical} {Systems}",
    volume = "15",
    issn = "1536-0040",
    url = "http://epubs.siam.org/doi/10.1137/151004896",
    doi = "10.1137/151004896",
    abstract = "Multi-time-scale stochastic dynamical systems are ubiquitous in science and engineering, and the reduction of such systems and their models to only their slow components is often essential for scientific computation and further analysis. Rather than being available in the form of an explicit analytical model, often such systems can only be observed as a data set which embodies dynamics on several time scales. We focus on applying and adapting data-mining and manifold learning techniques to detect the slow components in a class of such multiscale data. Traditional data-mining methods are based on metrics (and thus, geometries) which are not informed of the multiscale nature of the underlying system dynamics; such methods cannot successfully recover the slow variables. Here, we present an approach which utilizes both the local geometry and the local noise dynamics within the data set through a metric which is both insensitive to the fast variables and more general than simple statistical averaging. Our analysis of the approach provides conditions for successfully recovering the underlying slow variables, as well as an empirical protocol guiding the selection of the method parameters. Interestingly, the recovered underlying variables are gauge invariant{\textemdash}they are insensitive to the measuring instrument/observation function.",
    language = "en",
    number = "3",
    urldate = "2020-09-08",
    journal = "SIAM Journal on Applied Dynamical Systems",
    month = "January",
    year = "2016",
    pages = "1327--1351",
    file = {dsilva-2016-math:Articles\\dsilva-2016-math.pdf:PDF}
}

