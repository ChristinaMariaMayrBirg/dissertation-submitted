 doi = {10.1137/130916138},
 file = {constantine-2014b-math.pdf:Articles/constantine-2014b-math.pdf:PDF},
 journal = {SIAM Journal on Scientific Computing},
 keywords = {uncertainty quantification, active subspaces},
