    file = {le-2017-math:Articles\\le-2017-math.pdf:PDF}
}

@article{dsilva-2018-math,
    author = "Dsilva, Carmeline J. and Talmon, Ronen and Coifman, Ronald R. and Kevrekidis, Ioannis G.",
