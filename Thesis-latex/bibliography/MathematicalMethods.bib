 file = {coifman-2005-math.pdf:Articles\\coifman-2005-math.pdf:PDF},
 journal = {Proceedings of the National Academy of Sciences of the United States of America},
 keywords = {diffusion maps, mathematics, numerics},
 number = {21},
 pages = {7426--7431},
 title = {Geometric diffusions as a tool for harmonic analysis and structure definition of data: Diffusion maps},
 volume = {102},
